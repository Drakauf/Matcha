	link(href="https://fonts.googleapis.com/css?family=Poppins" rel="stylesheet")
	script(src="/js/photos.js")
	script(src="/js/login_modal.js")
	style
		include styles/base.css
		include styles/login_modal.css
		include styles/header.css
		include styles/footer.css
		include styles/navigation.css
		include styles/signup.css
		include styles/update.css
		include styles/photos.css
		include styles/blocked_modal.css
		include styles/profil.css
		include styles/home.css
		include styles/news.css
		include styles/chat.css
body
	.container-fluid#main
		.row#header
			include header.pug
		if sess.user
			.row#section
				script(src="/js/notifications.js")
				.col-md-2#navigation
					include navigation.pug
				.col-md-1
				.col-md-9#body
					block body-content
		else
			.row#register
				include signup.pug
		.row#footer
			.col-md-12
				include footer.pug
